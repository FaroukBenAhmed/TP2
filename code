import csv
import json
import math
import os
Class DonneesGeo:
  def __init__(self, ville, pays, latitude, longitude):
    self.ville = str(ville)
    self.pays = str(pays)
    self.latitude = float(latitude)
    self.longitude = float(longitude)

  def __str__(self):
    return f"{self.ville}, {self.pays}, {self.latitude}, {self.longitude}"
